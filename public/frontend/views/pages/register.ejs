<body class="h-100">
    <div class="authincation h-100">
        <div class="container h-100">
            <div class="row justify-content-center h-100 align-items-center">
                <div class="col-md-12">
					
					<div class="authincation-content">
                        <div class="row no-gutters">
                            <div class="col-xl-12">
                                <div class="auth-form">
									<div class="text-center mb-12">
										<!-- <a href="index.html"><img src="images/logo-full.png" alt=""></a> -->
									</div>
                                    <h4 class="text-center mb-1 text-white">Sign up your account</h4>
                                    <form action="/api/post/user/register" method="post">
                                        <div class="row" style="margin-top: 20px;">
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Email</strong></label>
                                                <input type="email" class="form-control" placeholder="hello@example.com" name="email">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Business Name</strong></label>
                                                <input type="text" class="form-control" placeholder="Business Name" name="businessName">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Nature Of Business</strong></label>
                                                <input type="text" class="form-control" placeholder="Nature Of Business" name="natureOfBusiness">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Business Type</strong></label>
                                                <input type="text" class="form-control" placeholder="Business Type" name="businessType">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Business Location</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="location">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Business Address</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="address">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Contact</strong></label>
                                                <input type="text" class="form-control" placeholder="contact" name="contact">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Busines Logo</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="businessLogo">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Country</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="country">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Region/state</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="region_state">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>Town</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="town">
                                            </div>
                                            <div class="form-group; col-md-6">
                                                <label class="mb-1 text-white"><strong>CEO Name</strong></label>
                                                <input type="text" class="form-control" placeholder="" name="ceo ">
                                            </div>
                                            <!-- <div class="form-group; col-md-4">
                                                <label class="mb-1 text-white"><strong>Role</strong></label>
                                                <input type="text" class="form-control" placeholder="">
                                            </div> -->
                                    
                                            <div class="form-group; col-md-4">
                                                <label class="mb-1 text-white"><strong>Password</strong></label>
                                                <input type="password" class="form-control" value="Password" name="new_pass">
                                            </div> 
                                            <div class="form-group; col-md-4">
                                                <label class="mb-1 text-white"><strong>Confirm Password</strong></label>
                                                <input type="password" class="form-control" value="Password" name="confirm_pass">
                                            </div> 
                                            
                                            <div class="text-center mt-4" style="margin-top: 10px;">
                                                <button type="submit" class="btn bg-white text-primary btn-block">Sign me up</button>
                                            </div>
                                        </div>
                                    </form>
                                    <div class="new-account mt-3">
                                        <p class="text-white">Already have an account? <a class="text-white" href="page-login.html">Sign in</a></p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

